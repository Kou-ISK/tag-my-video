{
  "name": "tag-my-video",
  "version": "3.0.0",
  "description": "Video tagging application for sports analysis",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/utils": "^5.16.6",
    "@mui/x-charts": "^7.18.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/recharts": "^1.8.29",
    "@types/video.js": "^7.3.58",
    "concurrently": "^9.0.1",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.1",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-datepicker": "^7.4.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "recharts": "^2.12.7",
    "redux": "^5.0.1",
    "typescript": "^5.4.5",
    "ulid": "^2.3.0",
    "video.js": "^8.18.1",
    "wait-on": "^8.0.1",
    "web-vitals": "^4.2.3"
  },
  "main": "build/electron/src/main.js",
  "homepage": "./",
  "author": "Kou-ISK",
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "output": "dist/"
    },
    "appId": "com.example.tagMyVideo",
    "productName": "Tag My Video",
    "icon": "public/icon.png"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "fix": "pnpm run lint && pnpm run format",
    "format": "prettier --write '**/*.ts' '**/*.tsx'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "pnpm run build && pnpm exec tsc -p electron && electron .",
    "electron:build": "pnpm run build && pnpm exec tsc -p electron && electron-builder",
    "electron:dev": "concurrently \"pnpm start\" \"wait-on http://localhost:3000 && pnpm run electron:start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/react-datepicker": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "electron": "^31.6.0",
    "electron-builder": "^25.0.5"
  },
  "repository": "https://github.com/Kou-ISK/tag-my-video.git",
  "license": "MIT"
}
